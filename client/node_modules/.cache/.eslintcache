rap\">\r\n            <div ref={mapContainer} className=\"map\" />\r\n        </div>\r\n    )\r\n}\r\n","C:\\Users\\adrean paul sorono\\Desktop\\Errand Catcher\\ErrandCatcher\\client\\src\\components\\HeroSection.js",[],[],"C:\\Users\\adrean paul sorono\\Desktop\\Errand Catcher\\ErrandCatcher\\client\\src\\components\\ButtonMap.js",[],[],"C:\\Users\\adrean paul sorono\\Desktop\\Errand Catcher\\ErrandCatcher\\client\\src\\reportWebVitals.js",[],[],"C:\\Users\\adrean paul sorono\\Desktop\\Errand Catcher\\ErrandCatcher\\client\\src\\components\\NavButton.js",[],[],"C:\\Users\\adrean paul sorono\\Desktop\\Errand Catcher\\ErrandCatcher\\client\\src\\pages\\Notification.jsx",[],[],"C:\\Users\\adrean paul sorono\\Desktop\\Errand Catcher\\ErrandCatcher\\client\\src\\components\\NotificationItem.js",[],[],{"ruleId":"215","severity":1,"message":"216","line":10,"column":12,"nodeType":"217","messageId":"218","endLine":10,"endColumn":21},{"ruleId":"215","severity":1,"message":"219","line":10,"column":23,"nodeType":"217","messageId":"218","endLine":10,"endColumn":35},{"ruleId":"220","severity":1,"message":"221","line":45,"column":6,"nodeType":"222","endLine":45,"endColumn":18,"suggestions":"223"},{"ruleId":"215","severity":1,"message":"224","line":30,"column":12,"nodeType":"217","messageId":"218","endLine":30,"endColumn":15},{"ruleId":"215","severity":1,"message":"225","line":30,"column":17,"nodeType":"217","messageId":"218","endLine":30,"endColumn":23},{"ruleId":"215","severity":1,"message":"226","line":31,"column":12,"nodeType":"217","messageId":"218","endLine":31,"endColumn":15},{"ruleId":"215","severity":1,"message":"227","line":31,"column":17,"nodeType":"217","messageId":"218","endLine":31,"endColumn":23},{"ruleId":"215","severity":1,"message":"228","line":79,"column":11,"nodeType":"217","messageId":"218","endLine":79,"endColumn":25},{"ruleId":"220","severity":1,"message":"229","line":136,"column":6,"nodeType":"222","endLine":136,"endColumn":21,"suggestions":"230"},{"ruleId"